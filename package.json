    "axios": "^0.16.2",
    "bootstrap-sass": "^3.3.7",
    "cross-env": "^5.0.1",
    "font-awesome": "^4.7.0",
    "laravel-mix": "^1.4.2",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "vue": "^2.4.2",
    "vue-axios": "^2.0.2",
    "vue-moment": "^2.0.2",
    "vue-strap": "github:wffranco/vue-strap"
